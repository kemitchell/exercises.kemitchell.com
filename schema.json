          "latissimus dorsi",
          "levator scapulae",
          "pectoralis major",
          "pectoralis minor",
          "quadricpes",
          "rectus abdominis",
          "rhomboids",
          "teres major",
          "teres minor",
          "transverse abdominis",
          "trapezius",
