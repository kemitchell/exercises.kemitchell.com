    "name",
    "type",
    "cues"
  ],
  "properties": {
    "name": {
      "type": "string",
      "minLength": 1
    },
    "type": {
      "enum": [
        "static",
        "dynamic"
      ]
    },
    "videos": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "required": [
          "url",
          "source"
        ],
        "additionalProperties": false,
        "properties": {
          "url": {
            "type": "string",
            "format": "url"
          },
          "source": {
            "enum": [
              "Tykato Fitness",
              "Fitness FAQs",
              "CrossFit",
              "Calisthenic Movement",
              "Antranik"
            ]
          }
        }
      }
    },
    "cues": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "string",
        "minLength": 1
      }
    },
    "progressions": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "string",
        "minLength": 1
      }
    }
  }
}
