{
  "type": "object",
  "additionalProperties": false,
  "required": [
    "name",
    "type",
    "cues"
  ],
  "properties": {
    "name": {
      "type": "string",
      "minLength": 1
    },
    "type": {
      "enum": [
        "static",
        "dynamic"
      ]
    },
    "chain": {
      "enum": [
        "open",
        "closed"
      ]
    },
    "muscles": {
      "type": "array",
      "minItems": 1,
      "items": {
        "enum": [
          "pectoralis major",
          "rectus abdominis",
          "transverse abdominis",
          "internal oblique",
          "external oblique",
          "deltoid",
          "latissimus dorsi",
          "biceps brachii",
          "brachioradialis",
          "triceps brachii",
          "quadricpes",
          "hamstrings"
        ]
      }
    },
    "videos": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "required": [
          "url",
          "source"
        ],
        "additionalProperties": false,
        "properties": {
          "url": {
            "type": "string",
            "format": "url"
          },
          "source": {
            "enum": [
              "Tykato Fitness",
              "Fitness FAQs",
              "CrossFit",
              "Calisthenic Movement",
              "Antranik"
            ]
          }
        }
      }
    },
    "cues": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "string",
        "minLength": 1
      }
    },
    "progressions": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "string",
        "minLength": 1
      }
    }
  }
}
